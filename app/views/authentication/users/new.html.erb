<%= form_with(model: @user) do |form| %>
    <% @user.errors.full_messages.each do |error| %>
      <div><%= error %></div>
    <% end %>
        
    <div>
      <%= form.label :emails %>
      <%= form.email_field :emails %>
    </div>

    <div>
      <%= form.label :username %>
      <%= form.text_field :username %>
    </div>
    
    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
<% end %>
  